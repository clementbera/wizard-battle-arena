r: 'accessing' stamp: 'cb 7/9/2015 10:09'!cell	^ cell! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 10:09'!cell: anObject	cell := anObject! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 11:32'!currentFoot	^ currentFoot! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 11:32'!currentFoot: anObject	currentFoot := anObject! !!WBACharacter methodsFor: 'animation' stamp: 'ClementBera 7/22/2015 16:40'!die	isMoving := true.	scheduler stopGameTickActionFor: self.	scheduler 		newGameTickAction: (WBADieAnimation new			character: self;			yourself)		for: self! !!WBACharacter methodsFor: 'animation' stamp: 'ClementBera 7/22/2015 16:32'!dieRotation	direction := direction \\ 4 + 1! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 10:09'!direction	^ direction! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 10:09'!direction: anObject	direction := anObject! !!WBACharacter methodsFor: 'animation' stamp: 'ClementBera 7/22/2015 16:15'!finishMoveAnimation 	scheduler stopGameTickActionFor: self.	isMoving := false.	player movementKeyDown		ifNil: [ currentFoot := 0 ]		ifNotNil: [ :dir | self maybeMove: dir ]! !!WBACharacter methodsFor: 'accessing' stamp: 'ClementBera 7/17/2015 15:21'!game	^ game! !!WBACharacter methodsFor: 'accessing' stamp: 'ClementBera 7/17/2015 15:21'!game: anObject	game := anObject! !!WBACharacter methodsFor: 'initialization' stamp: 'cb 7/9/2015 10:41'!initialShift	^ 8@ -32! !!WBACharacter methodsFor: 'initialization' stamp: 'ClementBera 2/1/2016 10:51'!initialize	super initialize.	self shift: self initialShift.	direction := 1.	isMoving := false.	currentFoot := 0.! !!WBACharacter methodsFor: 'accessing' stamp: 'ClementBera 7/17/2015 15:22'!isDead	^ cell isNil! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 10:09'!isMoving	^ isMoving! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 10:09'!isMoving: anObject	isMoving := anObject! !!WBACharacter methodsFor: 'animation' stamp: 'ClementBera 7/23/2015 16:58'!maybeMove: index	isMoving ifTrue: [ "already moves" ^ self ].	direction := index. 	(cell neighbours at: index) element allowMovement ifFalse: [ ^ currentFoot := 0 ].	self move: index! !!WBACharacter methodsFor: 'animation' stamp: 'ClementBera 7/22/2015 16:27'!move: index	isMoving := true.	currentFoot := 1.	scheduler 		newGameTickAction: (WBACharacterMoveAnimation new			character: self;			direction: index;			yourself)		for: self ! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 10:09'!player	^ player! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 10:09'!player: anObject	player := anObject! !!WBACharacter methodsFor: 'rendering' stamp: 'ClementBera 2/1/2016 10:52'!renderOn: canvas	canvas pathTransform translateBy: shift.	canvas setPaint: self spriteToRender.	canvas draw.	canvas pathTransform translateBy: negatedShift. ! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 11:18'!scheduler	^ scheduler! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 11:18'!scheduler: anObject	scheduler := anObject! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 11:45'!spell	^ spell! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 11:45'!spell: anObject	spell := anObject! !!WBACharacter methodsFor: 'rendering' stamp: 'cb 7/9/2015 11:33'!spriteToRender	^ sprites at: direction * 3 - 1 + currentFoot 	! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 10:09'!sprites	^ sprites! !!WBACharacter methodsFor: 'accessing' stamp: 'cb 7/9/2015 10:09'!sprites: anObject	sprites := anObject! !!WBACharacter methodsFor: 'animation' stamp: 'ClementBera 2/1/2016 10:51'!switchCell: shiftIncr	| newCell |	newCell := cell neighbours at: direction.	newCell element allowMovement ifTrue: [ 		cell element: WBAEmpty empty.		cell := newCell.		cell element: self.		cell spells 			detect: [ :sp | sp character ~=