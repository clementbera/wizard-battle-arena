((index \\ 3 - 1) @ (index // 2 - 1)) negated.	newLocation := (newLocation max: 0@0) min: 12@7.	(currentChoices anySatisfy: [ :each | each = newLocation ]) ifTrue: [ ^ self ].	currentChoices at: playerIndex put: newLocation.! !!WBACharacterSelectScreenModel methodsFor: 'actions' stamp: 'ClementBera 7/17/2015 17:13'!hasPlayerSpecificHandling	^ true! !!WBACharacterSelectScreenModel methodsFor: 'initialization' stamp: 'ClementBera 7/17/2015 17:53'!loadWidgets	| numPlayers |	titleWidget := self newSelectWidget: (self textWidgetAt: #character).	characterWidget := self newCharacterWidget.	numPlayers := startingGame players size.	arrowWidgets := self newCadreWidgets: numPlayers.	currentChoices := { 0@0 . 2@2 . 4@4 . 6@6 } copyFrom: 1 to: numPlayers.	! !!WBACharacterSelectScreenModel methodsFor: 'screen building' stamp: 'ClementBera 7/17/2015 18:03'!newCharacterRowWidget: i 	^ self newRowWidget: (self characterWidgets: i) space: 52! !!WBACharacterSelectScreenModel methodsFor: 'screen building' stamp: 'ClementBera 7/17/2015 17:59'!newCharacterWidget	^ self newColumnWidget: self characterRowWidgets space: 50! !!WBACharacterSelectScreenModel methodsFor: 'rendering' stamp: 'ClementBera 7/17/2015 17:50'!renderWidgetsOn: canvas	| activeChoices |	activeChoices := currentChoices. "can change while rendering"	titleWidget renderOn: canvas translatedBy: 210@30.	characterWidget renderOn: canvas translatedBy: -160@70.	canvas pathTransform translateX: -5 Y: -2.	arrowWidgets withIndexDo: [ :w :i |		w renderOn: canvas temporarilyTranslatedBy: (activeChoices at: i) * (52@50) ] ! !!WBACharacterSelectScreenModel methodsFor: 'rendering' stamp: 'ClementBera 7/17/2015 17:34'!startAnimation	self every10GameTickDo: [		characterWidget widgets do: [ :w |			w widgets do: [ :toggleSp |				toggleSp toggle ] ] ] ! !!WBACharacterSelectScreenModel methodsFor: 'actions' stamp: 'ClementBera 7/17/2015 17:27'!switchToNextScreen			self nextScreen: WBASpellSelectScreenModel ! !!WBAEndScreenModel methodsFor: 'actions' stamp: 'ClementBera 7/17/2015 15:50'!downSelection	currentChoice := currentChoice + 1 \\ 2! !!WBAEndScreenModel methodsFor: 'initialization' stamp: 'ClementBera 7/17/2015 15:50'!initialize	super initialize.	currentChoice := 0.! !!WBAEndScreenModel methodsFor: 'initialization' stamp: 'ClementBera 7/17/2015 18:00'!loadWidgets	titleWidget := self textWidgetAt: #winner.		spellWidget := self newAnimatedWidget: (winner character spell sprites last: 4).			characterWidget := self newAnimatedWidget: { 		winner character sprites first .		winner character sprites third }.			arrowWidget := self newLargeArrowSelectWidget: 450.		choiceWidgets := self 		newColumnWidget: { 			self newSelectWidget: (self textWidgetAt: #arena) .			self newSelectWidget: (self textWidgetAt: #players) } 		space: 80	! !!WBAEndScreenModel methodsFor: 'rendering' stamp: 'ClementBera 7/17/2015 15:53'!renderWidgetsOn: canvas	titleWidget renderOn: canvas translatedBy: 260@30.	spellWidget renderOn: canvas translatedBy: -12@90.	characterWidget renderOn: canvas translatedBy: 90@0.	spellWidget renderOn: canvas translatedBy: 90@0.	choiceWidgets renderOn: canvas translatedBy: -220@90.	arrowWidget renderOn: canvas translatedBy: -90 @ (10 + (80 * currentChoice)).! !!WBAEndScreenModel methodsFor: 'animation' stamp: 'ClementBera 7/17/2015 17:34'!startAnimation	self every10GameTickDo: [ 		characterWidget toggle.		spellWidget toggle ] ! !!WBAEndScreenModel methodsFor: 'actions' stamp: 'ClementBera 7/17/2015 18:27'!switchToNextScreen	currentChoice = 0 		ifTrue: [ self nextScreen: WBAArenaSelectScreenModel ]		ifFalse: [ 			startingGame scheduler terminateProcess.			WBANumberPlayerSelectScreenModel new				screen: screen;				display ]! !!WBAEndScreenModel methodsFor: 'actions' stamp: 'ClementBera 7/17/2015 15:51'!upSelection	currentChoice := currentChoice + 1 \\ 2! !!WBAEndScreenModel methodsFor: 'initialization' stamp: 'Clem