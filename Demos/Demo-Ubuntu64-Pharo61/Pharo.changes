reateFormPaint: (genForm copy: ((0 @ 192 + position) corner: (48 @ 224 + position)))).	grounds at: 2 put: (surface createFormPaint: (genForm copy: ((0 @ 240 + position) corner: (48 @ 272 + position)))).	^ grounds! !!WBABackgroundDescriptor methodsFor: 'bootstrap' stamp: 'cb 7/9/2015 10:26'!loadWallSpritesWith: surface	| genForm walls |	walls := Array new: 3.	genForm := self loadImageFile.		0 to: 2 do: [ :i | | topLeftCorner subRect |		topLeftCorner := 0 @ i * self wallSpriteSize + position.		subRect := topLeftCorner corner: self wallSpriteSize + topLeftCorner.		walls at: i + 1 put: (surface createFormPaint: (genForm copy: subRect)) ].	^ walls! !!WBABackgroundDescriptor methodsFor: 'accessing' stamp: 'cb 7/8/2015 21:07'!wallSpriteSize	^ 48 @ 64! !!WBACadreDescriptor methodsFor: 'as yet unclassified' stamp: 'ClementBera 7/16/2015 16:46'!classFolder	^ 'perso/'! !!WBACadreDescriptor methodsFor: 'as yet unclassified' stamp: 'ClementBera 7/16/2015 16:47'!loadSpritesWith: surface numSprites: num	| sprites |	sprites := Array new: num.	1 to: num do: [ :i |		fileName := 'cadre' , i printString.		sprites at: i put: self loadImageFile ].	^ sprites! !!WBACharacterDescriptor class methodsFor: 'instance creation' stamp: 'ClementBera 7/9/2015 19:29'!bestCharacterAt: i	"Set of good looking wizard style characters"	| indexes |	indexes := #(	#(1 288 0)	#(5 0 0)	#(9 0 192)	#(9 288 0)	) at: i.	^ self new		fileName: 'perso' , indexes first printString;		position: indexes second @ indexes third;		yourself! !!WBACharacterDescriptor class methodsFor: 'instance creation' stamp: 'cb 7/8/2015 17:36'!randomCharacter	^ self new		fileName: 'perso' , 13 atRandom printString;		position: #(0 96 192 288) atRandom @ #(0 192) atRandom ;		yourself! !!WBACharacterDescriptor methodsFor: 'accessing' stamp: 'ClementBera 7/10/2015 11:50'!classFolder	^ 'perso/'! !!WBACharacterDescriptor methodsFor: 'bootstrap' stamp: 'ClementBera 7/16/2015 15:04'!loadShortSpritesWith: surface	| genForm sprites |	genForm := self loadImageFile.	sprites := Array new: 2.	0 to: 1 do: [ :i | | topLeftCorner |		topLeftCorner := i @ 0 * (self spriteSize * 2) + position.		sprites 			at:  i + 1 			put: (surface createFormPaint: (genForm copy: (topLeftCorner corner: self spriteSize + topLeftCorner))) ].	^ sprites! !!WBACharacterDescriptor methodsFor: 'bootstrap' stamp: 'cb 7/8/2015 21:23'!loadSpritesWith: surface	| genForm sprites |	genForm := self loadImageFile.	sprites := Array new: 12.	0 to: 3 do: [ :j | 		0 to: 2 do: [ :i | | topLeftCorner |			topLeftCorner := i @ j * self spriteSize + position.			sprites 				at: j * 3 + i + 1 				put: (surface createFormPaint: (genForm copy: (topLeftCorner corner: self spriteSize + topLeftCorner))) ] ].	^ sprites! !!WBASpellDescriptor class methodsFor: 'instance creation' stamp: 'ClementBera 7/16/2015 19:44'!randomSpell	^ self new		fileName: 'spell' , 6 atRandom printString;		position: 0@0 ;		yourself! !!WBASpellDescriptor methodsFor: 'accessing' stamp: 'ClementBera 7/10/2015 11:50'!classFolder	^ 'spell/'! !!WBASpellDescriptor methodsFor: 'bootstrap' stamp: 'ClementBera 7/17/2015 14:27'!loadExplodeSpritesWith: surface	| sprites genForm |	genForm := self loadImageFile.	sprites := Array new: 3.	4 to: 6 do: [ :i | | point |		point := i - 1 @ 0 * self spriteSize.		sprites at: i - 3 put: (surface createFormPaint: (genForm copy: (point corner: self spriteSize + point))) ].	^ sprites! !!WBASpellDescriptor methodsFor: 'bootstrap' stamp: 'cb 7/8/2015 21:23'!loadSpritesWith: surface	| sprites genForm |	genForm := self loadImageFile.	sprites := Array new: 6.	1 to: 6 do: [ :i | | point |		point := i - 1 @ 0 * self spriteSize.		sprites at: i put: (surface createFormPaint: (genForm copy: (point corner: self spriteSize + point))) ].	^ sprites! !!WBASpritesDescriptor class methodsFor: 'class initialization' stamp: 'ClementBera 7/16/2015 15:39'!initialize	self newCache! !!W