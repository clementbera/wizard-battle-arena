cessing' stamp: 'ClementBera 7/16/2015 14:27'!space	^ space! !!WBAColumnWidget methodsFor: 'accessing' stamp: 'ClementBera 7/16/2015 14:27'!space: anObject	space := anObject! !!WBAColumnWidget methodsFor: 'accessing' stamp: 'ClementBera 7/16/2015 14:24'!widgets	^ widgets! !!WBAColumnWidget methodsFor: 'accessing' stamp: 'ClementBera 7/16/2015 14:24'!widgets: anObject	widgets := anObject! !!WBAArenaSelectScreenModel methodsFor: 'actions' stamp: 'ClementBera 7/17/2015 15:11'!downSelection	currentChoice := currentChoice \\ self numArenas + 1.! !!WBAArenaSelectScreenModel methodsFor: 'actions' stamp: 'ClementBera 7/17/2015 17:24'!endScreenComputation	startingGame gameDescriptor arenaDescriptor: (WBAArenaDescriptor perform: (arenas at: currentChoice)).! !!WBAArenaSelectScreenModel methodsFor: 'initialization' stamp: 'ClementBera 7/17/2015 14:53'!initialize	super initialize.	currentChoice := 1.! !!WBAArenaSelectScreenModel methodsFor: 'initialization' stamp: 'ClementBera 7/17/2015 15:49'!loadWidgets	titleWidget := self newSelectWidget: (self textWidgetAt: #arena).	arenas := WBAArenaDescriptor perform: ('available' , startingGame players size printString, 'Players') asSymbol.	arenaWidget := self newArenaWidget: self numArenas.	arrowWidget := self newLargeArrowSelectWidget: 300.! !!WBAArenaSelectScreenModel methodsFor: 'screen building' stamp: 'ClementBera 7/17/2015 17:59'!newArenaWidget: numArenas	^ self newColumnWidget: ((1 to: numArenas) collect: [ :i | self newSubArenaWidget: i ]) space: 80! !!WBAArenaSelectScreenModel methodsFor: 'screen building' stamp: 'ClementBera 7/17/2015 18:03'!newSubArenaWidget: num	^ self newRowWidget: { (self textWidgetAt: #arena) . (self textWidgetAt: num) } space: 170! !!WBAArenaSelectScreenModel methodsFor: 'accessing' stamp: 'ClementBera 7/17/2015 15:11'!numArenas	^ arenas size min: 4! !!WBAArenaSelectScreenModel methodsFor: 'rendering' stamp: 'ClementBera 7/17/2015 15:11'!renderWidgetsOn: canvas	t